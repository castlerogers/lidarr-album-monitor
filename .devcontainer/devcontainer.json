{
    "name": "PowerShell Development",
    "dockerFile": "./Dockerfile.dev",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.powershell"
            ],
            "settings": {
                "terminal.integrated.defaultProfile.linux": "pwsh"
            }
        }
    },    "runArgs": [
        "--env-file", "${localWorkspaceFolder}/.devcontainer/devcontainer.env"
    ]
}